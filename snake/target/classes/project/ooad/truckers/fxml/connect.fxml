<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="960.0" style="-fx-background-color: silver;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.ooad.truckers.controller.ConnectController">

    <ImageView pickOnBounds="true" preserveRatio="true" x="188.0" y="40.0">
        <image>
            <Image url="@/project/ooad/truckers/connect/available_games.png" />
        </image>
    </ImageView>

    <ListView fx:id="availableGamesListView" layoutX="188" layoutY="145" onMouseClicked="#listElementClicked" prefHeight="340.0" prefWidth="585.0" style="-fx-border-color: black; -fx-border-width: 2px" />

    <ImageView fx:id="backButton" onMouseClicked="#backButtonPressed" pickOnBounds="true" preserveRatio="true" x="11.0" y="527.0">
        <image>
            <Image url="@/project/ooad/truckers/buttons/back.png" />
        </image>
    </ImageView>
</AnchorPane>
